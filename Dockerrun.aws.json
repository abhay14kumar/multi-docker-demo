{
    "AWSEBDockerrunVersion" :2,
    "containerDefinitions" : [
        {
            "name" : "client",
            "image" : "abhay14kumar/multi-client-demo",
            "hostName" : "client",
            "essential" : false,
            "memory": 128
        },
        {
            "name" : "server",
            "image" : "abhay14kumar/multi-server-demo",
            "hostName" : "api",
            "essential" : false,
            "memory": 128
        },
        {
            "name" : "worker",
            "image" : "abhay14kumar/multi-worker-demo",
            "hostName" : "worker",
            "essential" : false,
            "memory": 128
        },
        {
            "name" : "nginx",
            "image" : "abhay14kumar/multi-nginx-demo",
            "hostName" : "nginx",
            "essential" : true,
            "portMappings":[{
                "hostPort": 80,
                "containerPort":80

            }],
            "links":["client","server"],
            "memory": 128
        }

    ]
}